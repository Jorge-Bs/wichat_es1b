{
  "version": 2,
  "builds": [
    {
      "src": "llmservice/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "gatewayservice/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "webapp/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/llmservice/(.*)",
      "dest": "llmservice/$1"
    },
    {
      "src": "/gatewayservice/(.*)",
      "dest": "gatewayservice/$1"
    },
    {
      "src": "/webapp/(.*)",
      "dest": "webapp/$1"
    }
  ]
}
