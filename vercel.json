{
  "version": 2,
  "projects": [
    {
      "name": "llmservice",
      "src": "llmservice",
      "builds": [
        { "src": "llmservice/package.json", "use": "@vercel/node" }
      ],
      "routes": [
        { "src": "/llmservice/(.*)", "dest": "llmservice/$1" }
      ]
    },
    {
      "name": "gatewayservice",
      "src": "gatewayservice",
      "builds": [
        { "src": "gatewayservice/package.json", "use": "@vercel/node" }
      ],
      "routes": [
        { "src": "/gatewayservice/(.*)", "dest": "gatewayservice/$1" }
      ]
    }
  ]
}
